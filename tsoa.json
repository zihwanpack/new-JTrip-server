{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/presentation/controllers/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "src/swagger",
    "specVersion": 3,
    "basePath": "/",
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "access_token",
        "in": "cookie"
      }
    }
  },
  "routes": {
    "routesDir": "src/presentation/routers/generated"
  }
}
